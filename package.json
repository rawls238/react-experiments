{
  "name": "react-experiments",
  "version": "4.1.0",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.0.1",
    "babel-loader": "^6.0.0",
    "babel-preset-jest": "^13.0.0",
    "webpack": "^1.5.3",
    "babel-jest": "12.1.0",
    "planout": "^3.0.0",
    "jest-cli": "~0.5.8",
    "react-addons-test-utils": "^0.14.3",
    "babel-plugin-add-module-exports": "0.2.1"
  },
  "description": "react-experiments is a Javascript library that assists in defining and managing UI experiments in React.",
  "main": "dist/react-experiments.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/HubSpot/react-experiments"
  },
  "keywords": [
    "ab",
    "a/b",
    "split testing",
    "ui experiments",
    "experiments react",
    "react-component",
    "component"
  ],
  "authors": [
    "Gustavo Vargas",
    "Guy Aridor",
    "Matthew Rheault"
  ],
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "dist/",
      "src/",
      "__tests__/utils",
      "node_modules/"
    ]
  },
  "scripts": {
    "test": "sh build.sh && jest",
    "build": "sh build.sh"
  },
  "license": "MIT"
